generator client {
  provider = "prisma-client-js"
  output   = "../generated/auth-client"
}

datasource db {
  provider = "postgresql"
}

model Usuario {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String

  @@map("usuarios")
}

model Rol {
  id     Int    @id @default(autoincrement())
  nombre String @unique

  @@map("roles")
}

model Permiso {
  id     Int    @id @default(autoincrement())
  nombre String @unique

  @@map("permisos")
}
